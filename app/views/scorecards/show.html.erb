<p id="notice"><%= notice %></p>

<p>
  <strong>Url:</strong>
  <%= @scorecard.url %>
</p>
<div class="bs-example" data-example-id="condensed-table"> 
	<table class="table table-condensed">
	<thead> <tr> <th>Name</th> <th>status</th> <th>Runs</th> <th>Balls</th><th>fours</th><th>sixes</th><th>strik rate</th> </tr> </thead> 
		<tbody>  
			<% @scorecard.battings.each do |batting| %>
			<tr>
				<th><%= batting.name %></th><td><%= batting.status %></td><td><%= batting.runs %></td><td><%= batting.balls %></td><td><%= batting.fours %></td><td><%= batting.sixes %></td><td><%= batting.strik_rake %></td>
			</tr>
			<% end %>
		</tbody> 
	</table> 
</div>